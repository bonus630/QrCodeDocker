<?xml version="1.0" encoding="utf-8"?>
<!--https://www.tutorialspoint.com/pg/xml/xml_character_entities.htm

	< (less-than)                   -   &lt;

	> (greater-than)                -   &gt;

	& (ampersand)                   -   &amp;

	' (apostrophe or single quote)  -   &apos;

	" (double-quote)                -   &quot;
	-->
<Schemes>
	<Scheme>
		<InvariablePart>instagram://user?username={0}</InvariablePart>
		<Image>instagran.png</Image>
		<Name>Instagran</Name>
		<Attributes>
			<Attribute type="string">User</Attribute>
		</Attributes>
	</Scheme>
	<Scheme>
		<InvariablePart>twitter://user?id={0}</InvariablePart>
		<Image>twitter.png</Image>
		<Name>Twitter</Name>
		<Attributes>
			<Attribute type="string">User</Attribute>
		</Attributes>
	</Scheme>
	<Scheme>
		<InvariablePart>https://api.whatsapp.com/send?phone={0}{1}{2}&amp;text={3}</InvariablePart>
		<Image>whatsapp.png</Image>
		<Name>Whatsapp</Name>
		<Attributes>
			<Attribute type="string">DDI</Attribute>
			<Attribute type="string">DDD</Attribute>
			<Attribute type="string">TEL</Attribute>
			<Attribute type="string">MSG</Attribute>
		</Attributes>
	</Scheme>
	<Scheme>
		<InvariablePart>tel://{0}{1}{2}</InvariablePart>
		<Image>tel.png</Image>
		<Name>Phone</Name>
		<Attributes>
			<Attribute type="string">DDI</Attribute>
			<Attribute type="string">DDD</Attribute>
			<Attribute type="string">Phone</Attribute>
		</Attributes>
	</Scheme>
	<Scheme>
		<InvariablePart>https://www.snapchat.com/add/{0}</InvariablePart>
		<Image>snapchat.png</Image>
		<Name>Snapchat</Name>
		<Attributes>
			<Attribute type="string">User</Attribute>
		</Attributes>
	</Scheme>
	<Scheme>
		<InvariablePart>https://t.me/{0}</InvariablePart>
		<Image>telegran.png</Image>
		<Name>Telegran</Name>
		<Attributes>
			<Attribute type="string">User</Attribute>
		</Attributes>
	</Scheme>
	
</Schemes>