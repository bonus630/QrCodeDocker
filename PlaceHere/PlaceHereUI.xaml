<UserControl x:Class="br.corp.bonus630.plugin.PlaceHere.PlaceHereUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:br.corp.bonus630.plugin.PlaceHere"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="284">

    <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
        <Label Content="Anchor:"/>
        <local:AnchorButton Width="30" Height="30" FactorChanged="AnchorButton_FactorChanged"></local:AnchorButton>
        <Button x:Name="btn_start" Content="Start" Height="30" Margin="20,0,20,0" Click="btn_start_Click"/>
    </StackPanel>

</UserControl>
