<UserControl x:Class="br.corp.bonus630.plugin.PlaceHere.PlaceHereUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:br.corp.bonus630.plugin.PlaceHere"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="284">

    <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
        <Label Content="{Binding LBA_ObjOrigin}"/>
        <local:AnchorButton Width="30" Height="30" FactorChanged="AnchorButton_FactorChanged" x:Name="anchorButton"></local:AnchorButton>
        <Button x:Name="btn_start" Content="{Binding BTN_Start}" Height="30" Margin="20,0,20,0" Click="btn_start_Click"/>
        <Button x:Name="btn_teste" Content="teste" Height="30" Margin="20,0,20,0" Click="btn_teste_click"/>
        <CheckBox Content="{Binding CB_GetContainer}" x:Name="ck_getContainer" VerticalAlignment="Center" Click="ck_getContainer_Click"/>
    </StackPanel>

</UserControl>
