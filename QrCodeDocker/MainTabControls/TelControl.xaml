<TabItem x:Class="br.corp.bonus630.QrCodeDocker.MainTabControls.TelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:br.corp.bonus630.QrCodeDocker.MainTabControls"
             mc:Ignorable="d" 
             >
    <TabItem.Resources>
        <XmlDataProvider x:Key="Lang" XPath="/root" />
    </TabItem.Resources>
    <Grid>
        <Label Name="lbaPhoneField" Content="{Binding Source={StaticResource Lang},XPath=LabelPhoneField}"/>
        <TextBox TextWrapping="Wrap" x:Name="txt_tel"  Margin="2,25,0,0" TextChanged="txt_tel_TextChanged" PreviewTextInput="txt_tel_PreviewTextInput"  VerticalAlignment="Top"/>
    </Grid>
</TabItem>
