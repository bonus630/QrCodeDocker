<UserControl x:Class="br.corp.bonus630.plugin.ShapeToCode.ShapeToCodeUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:br.corp.bonus630.plugin.ShapeToCode"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="284">
    <StackPanel>
        <Button Height="28" Content="Process Document" x:Name="btn_Doc" Margin="10,10,10,0" />
        <Button Height="28" Content="Process Page" x:Name="btn_Page" Margin="10,10,10,0" />
        <Button Height="28" Content="Process Selection" x:Name="btn_Selection" Margin="10,10,10,0" />
        <CheckBox Content="Delete Original" x:Name="ck_deleteOri" Margin="10,10,10,0"/>
        <CheckBox Content="Use Width to size" x:Name="ck_overWidth" Margin="10,10,10,0"/>
    </StackPanel>
</UserControl>
